def game_state(key)
  base_url = "http://no-limit-code-em.com/sandbox/players/"
  request_url = key + base_url
  url = URI.parse(request_url)
  req = Net::HTTP::Get.new(url.path)
  res = Net::HTTP.start(url.host, url.port) {|http|
    http.request(req)
  }
  puts res.body
end

game_state('fc2455f8-1879-4ad4-8012-1f337c2869f2')